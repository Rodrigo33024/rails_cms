<nav class="navbar navbar-light bg-faded">
  <h3>AdminUser#index</h3>
</nav>

<div>
  <%= link_to("Add New Admin User", {:action => 'new'}, :class => "btn btn-secondary action new") %>
</div>

<table class="table">
  <thead>
    <tr>
      <th>User</th>
      <th>Email</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th>Actions</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @adminUsers.each do |user| %>
        <tr>
          <th scope="row"><%= user.name %></th>
          <th><%= mail_to(user.email) %></th>
          <th><%= user.created_at %></th>
          <th><%= user.updated_at %></th>
          <td><%= link_to("Edit", {:action => 'edit', :id => user.id}, :class => "action edit") %></td>
          <td><%= link_to("Delete", {:action => 'delete', :id => user.id}, :class => "action delete") %></td>
        </tr>
    <% end %>
  </tbody>
</table>
