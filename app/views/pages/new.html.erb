<nav class="navbar navbar-light bg-faded">
  <h4>Pages New</h4>
</nav>

<p style="margin: 20px 0">
<%= link_to("<< Back to List", {:action => 'index', :subject_id => @subject.id}, :class => 'btn btn-secondary back-link') %>
</p>


<div class="row">
  <h4>Create a Page</h4>

<%= form_for(:page, :url => {:action => 'create', :subject_id => @subject.id}) do |f| %>
    <%= render(:partial =>"form", :locals => {:f => f}) %>
    <%= submit_tag("Create Subject", class: "btn btn-primary")%>
<% end %>

</div>
